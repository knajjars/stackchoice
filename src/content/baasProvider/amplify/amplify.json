{
  "$schema": "../../../../.astro/collections/baasProvider.schema.json",
  "slug": "amplify",
  "website": "https://aws.amazon.com/amplify/",
  "name": "AWS Amplify",
  "description": "AWS Amplify is one of the most complete backend as a service platforms that provides a set of tools and services for mobile and front-end web developers to build secure, scalable full stack applications, powered by AWS.",
  "shortDescription": "A comprehensive BaaS platform powered by AWS for building scalable and secure full-stack applications.",
  "logo": "./amplify.png",
  "sdks": [
    "React",
    "Angular",
    "Vue",
    "JavaScript",
    "React Native",
    "Flutter",
    "Android",
    "Swift"
  ],
  "selfHosted": false,
  "cloudHosted": true,
  "pricing": {
    "freeTierLimits": {
      "teamMembers": 1,
      "projects": 1,
      "authUsers": 1,
      "dbStorage": 1,
      "dbBandwidth": 1,
      "fileStorage": 1,
      "fileBandwidth": 1,
      "functionCalls": 1
    },
    "overagePricing": {
      "teamMembers": 1,
      "projects": 1,
      "authUsers": 1,
      "dbStorage": 1,
      "dbBandwidth": 1,
      "fileStorage": 1,
      "fileBandwidth": 1,
      "functionCalls": 1
    },
    "pricingTier": {
      "monthlyPrice": 1,
      "teamMembers": 1,
      "projects": 1,
      "authUsers": 1,
      "dbStorage": 1,
      "dbBandwidth": 1,
      "fileStorage": 1,
      "fileBandwidth": 1,
      "functionCalls": 1
    }
  },
  "features": {
    "authentication": {
      "social": true,
      "email": true,
      "phone": true,
      "magicLinks": true,
      "sso": true,
      "userManagement": true
    },
    "database": {
      "relations": true,
      "migrations": true,
      "search": true
    },
    "realtime": {
      "pubsub": true,
      "realtimeDatabase": true
    },
    "functions": {
      "triggers": true,
      "runtime": [
        "nodejs"
      ]
    },
    "storage": {
      "fileUpload": true,
      "cdn": true,
      "imageProcessing": true
    },
    "vectorDatabase": {
      "embedding": false,
      "similarity": false
    },
    "jobs": {
      "queueing": true,
      "scheduling": true
    },
    "pushNotifications": {
      "ios": true,
      "android": true
    },
    "permissions": {
      "rbac": true,
      "customRules": true,
      "rowLevelSecurity": true
    },
    "compliances": {
      "hipaa": true,
      "gdpr": true,
      "soc2": true,
      "pci": true
    }
  }
}