{
  "slug": "amplify",
  "website": "https://aws.amazon.com/amplify/",
  "name": "AWS Amplify",
  "description": "AWS Amplify is one of the most complete backend as a service platforms that provides a set of tools and services for mobile and front-end web developers to build secure, scalable full stack applications, powered by AWS.",
  "logo": "./amplify.png",
  "sdks": [],
  "features": {
    "authentication": {
      "social": true,
      "email": true,
      "phone": true,
      "magicLinks": true,
      "sso": true,
      "userManagement": true
    },
    "database": {
      "relations": true,
      "migrations": true,
      "search": true
    },
    "realtime": {
      "pubsub": true,
      "realtimeDatabase": true
    },
    "functions": {
      "triggers": true,
      "runtime": [
        "nodejs"
      ]
    },
    "selfHosted": {
      "repository": ""
    },
    "storage": {
      "fileUpload": true,
      "cdn": true,
      "imageProcessing": true
    },
    "vectorDatabase": {
      "embedding": false,
      "similarity": false
    },
    "jobs": {
      "queueing": true,
      "scheduling": true
    },
    "pushNotifications": {
      "ios": true,
      "android": true
    },
    "permissions": {
      "rbac": true,
      "customRules": true,
      "rowLevelSecurity": true
    },
    "compliances": {
      "hipaa": true,
      "gdpr": true,
      "soc2": true,
      "pci": true
    }
  }
}