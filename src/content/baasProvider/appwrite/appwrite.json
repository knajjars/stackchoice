{
  "$schema": "../../../../.astro/collections/baasProvider.schema.json",
  "slug": "appwrite",
  "website": "https://appwrite.io/",
  "name": "Appwrite",
  "description": "Appwrite is an open-source platform that provides authentication, databse, functions, storage and messaging for your project using your framework of choice.",
  "logo": "./appwrite.png",
  "repository": "https://github.com/appwrite/appwrite",
  "sdks": [
    "JavaScript",
    "Flutter",
    "React Native",
    "Swift",
    "Kotlin",
    "Node",
    "Python",
    "Dart",
    "PHP",
    "Ruby",
    ".NET",
    "Deno",
    "Go"
  ],
  "features": {
    "authentication": {
      "social": true,
      "email": true,
      "phone": true,
      "magicLinks": true,
      "sso": false,
      "userManagement": true
    },
    "database": {
      "relations": true,
      "migrations": false,
      "search": true
    },
    "realtime": {
      "pubsub": true,
      "realtimeDatabase": true
    },
    "functions": {
      "triggers": true,
      "runtime": [
        "node",
        "bun",
        "deno",
        "go",
        "python",
        "dart",
        "php",
        "ruby",
        ".net",
        "java",
        "swift",
        "kotlin",
        "c++"
      ]
    },
    "hosting": {
      "selfHosting": true,
      "cloudHosting": true
    },
    "vectorDatabase": {
      "embedding": false,
      "similarity": false
    },
    "storage": {
      "fileUpload": true,
      "cdn": true,
      "imageProcessing": true
    },
    "jobs": {
      "queueing": false,
      "scheduling": true
    },
    "pushNotifications": {
      "ios": true,
      "android": true
    },
    "permissions": {
      "rbac": true,
      "customRules": true,
      "rowLevelSecurity": true
    },
    "compliances": {
      "hipaa": true,
      "gdpr": true,
      "soc2": true,
      "pci": true
    }
  }
}