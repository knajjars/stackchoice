{
  "$schema": "../../../../.astro/collections/baasProvider.schema.json",
  "slug": "convex",
  "website": "https://convex.dev/",
  "name": "Convex",
  "description": "Convex is an open-source backend for application builders that want to focus on building their applications without having to worry about the underlying infrastructure.",
  "logo": "./convex.png",
  "repository": "https://github.com/get-convex/convex-backend",
  "sdks": [
    "React",
    "React Native",
    "JavaScript",
    "Svelte",
    "Python",
    "Rust"
  ],
  "cloudHosted": true,
  "selfHosted": true,
  "features": {
    "authentication": {
      "social": true,
      "email": true,
      "phone": true,
      "magicLinks": true,
      "sso": false,
      "userManagement": false
    },
    "database": {
      "relations": true,
      "migrations": true,
      "search": true
    },
    "realtime": {
      "pubsub": true,
      "realtimeDatabase": true
    },
    "functions": {
      "triggers": false,
      "runtime": [
        "node",
        "convex"
      ]
    },
    "storage": {
      "fileUpload": true,
      "cdn": true,
      "imageProcessing": false
    },
    "vectorDatabase": {
      "embedding": true,
      "similarity": true
    },
    "jobs": {
      "queueing": true,
      "scheduling": true
    },
    "pushNotifications": {
      "ios": false,
      "android": false
    },
    "permissions": {
      "rbac": false,
      "customRules": true,
      "rowLevelSecurity": false
    },
    "compliances": {
      "hipaa": true,
      "gdpr": true,
      "soc2": true,
      "pci": true
    }
  }
}