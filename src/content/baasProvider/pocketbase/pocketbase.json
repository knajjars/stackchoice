{
  "slug": "pocketbase",
  "website": "https://pocketbase.io/",
  "name": "Pocketbase",
  "description": "PocketBase is an open source backend consisting of embedded database (SQLite) with realtime subscriptions, built-in auth management, convenient dashboard UI and simple REST-ish API.",
  "logo": "./pocketbase.png",
  "repository": "https://github.com/pocketbase/pocketbase",
  "sdks": [
    "Go",
    "JavaScript"
  ],
  "features": {
    "authentication": {
      "social": true,
      "email": true,
      "phone": false,
      "magicLinks": false,
      "sso": false,
      "userManagement": true
    },
    "database": {
      "relations": true,
      "migrations": true,
      "search": true
    },
    "realtime": {
      "pubsub": true,
      "realtimeDatabase": true
    },
    "functions": {
      "triggers": false,
      "runtime": [
        "go",
        "node"
      ]
    },
    "hosting": {
      "selfHosting": true,
      "cloudHosting": false
    },
    "storage": {
      "fileUpload": true,
      "cdn": false,
      "imageProcessing": false
    },
    "vectorDatabase": {
      "embedding": false,
      "similarity": false
    },
    "jobs": {
      "queueing": false,
      "scheduling": true
    },
    "pushNotifications": {
      "ios": false,
      "android": false
    },
    "permissions": {
      "rbac": true,
      "customRules": true,
      "rowLevelSecurity": true
    },
    "compliances": {
      "hipaa": true,
      "gdpr": true,
      "soc2": true,
      "pci": true
    }
  }
}