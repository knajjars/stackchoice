{
  "$schema": "../../../../.astro/collections/baasProvider.schema.json",
  "slug": "supabase",
  "website": "https://supabase.com/",
  "name": "Supabase",
  "description": "Supabase is an open-source Firebase alternative. Supabase is a set of open-source tools and services that enables mobile and front-end web developers to build secure, scalable full stack applications, powered by Postgres.",
  "shortDescription": "An open-source Firebase alternative with a Postgres database, authentication, instant APIs, realtime subscriptions, and storage.",
  "logo": "./supabase.png",
  "repository": "https://github.com/supabase/supabase",
  "sdks": [
    "JavaScript",
    "C#",
    "Flutter",
    "Swift",
    "Python",
    "Kotlin"
  ],
  "selfHosted": true,
  "cloudHosted": true,
  "pricing": {
    "freeTierLimits": {
      "teamMembers": "unlimited",
      "projects": 2,
      "authUsers": 50000,
      "dbStorage": 0.5,
      "dbBandwidth": 5,
      "fileStorage": 1,
      "fileBandwidth": 5,
      "functionCalls": 500000,
      "note": "Database and file bandwidth are shared."
    },
    "pricedTier": {
      "monthlyPrice": 25,
      "teamMembers": "unlimited",
      "projects": 1,
      "authUsers": 100000,
      "dbStorage": 8,
      "dbBandwidth": 250,
      "fileStorage": 100,
      "fileBandwidth": 250,
      "functionCalls": 2000000,
      "note": "Database and file bandwidth are shared."
    },
    "overagePricing": {
      "authUsers": 0.00325,
      "dbStorage": 0.125,
      "dbBandwidth": 0.09,
      "fileStorage": 0.021,
      "fileBandwidth": 0.09,
      "functionCalls": 2,
      "note": "Database and file bandwidth are shared."
    }
  },
  "features": {
    "authentication": {
      "social": true,
      "email": true,
      "phone": true,
      "magicLinks": true,
      "sso": true,
      "userManagement": true
    },
    "database": {
      "relations": true,
      "migrations": true,
      "search": true
    },
    "realtime": {
      "pubsub": true,
      "realtimeDatabase": true
    },
    "functions": {
      "triggers": true,
      "runtime": [
        "deno"
      ]
    },
    "storage": {
      "fileUpload": true,
      "cdn": true,
      "imageProcessing": true
    },
    "vectorDatabase": {
      "embedding": true,
      "similarity": true
    },
    "jobs": {
      "queueing": false,
      "scheduling": false
    },
    "pushNotifications": {
      "ios": false,
      "android": false
    },
    "permissions": {
      "rbac": true,
      "customRules": true,
      "rowLevelSecurity": true
    },
    "compliances": {
      "hipaa": true,
      "gdpr": true,
      "soc2": true,
      "pci": true
    }
  }
}